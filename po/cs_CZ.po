#: ../src/ch-flash.c:166
msgid "Connect to the internet to check for updates"
msgstr "Připojit k internetu pro kontrolu aktualizací"

#. TRANSLATORS: we can only set the flash success
#. * flag when the *new* firmware is running
#: ../src/ch-flash.c:212
msgid "Failed to set the flash success true"
msgstr "Nepodařilo se zaznamenat úspěšné nahrání"

#. TRANSLATORS: we've uploaded new firmware
#: ../src/ch-flash.c:221
msgid "Device successfully updated"
msgstr "Zařízení bylo úspěšně aktualizováno"

#: ../src/ch-flash.c:232
msgid "Calibration Completed"
msgstr "Kalibrace dokončena"

#. TRANSLATORS: tell the hardware we succeeded
#: ../src/ch-flash.c:249
msgid "Setting flash success..."
msgstr "Zaznamenávám úspěšné nahrání..."

#. TRANSLATORS: the device failed to boot up with the
#. * new firmware
#: ../src/ch-flash.c:277
msgid "Failed to startup the ColorHug"
msgstr "Nepodařilo se zapnout ColorHug"

#. TRANSLATORS: the new firmware will not load
#: ../src/ch-flash.c:307
msgid "Failed to boot the ColorHug"
msgstr "Nepodařilo se nabootovat ColorHug"

#. TRANSLATORS: tell the device the firmware is no
#. * longer known working
#: ../src/ch-flash.c:341
msgid "Failed to verify the firmware"
msgstr "Nepodařilo se ověřit firmware."

#. TRANSLATORS: boot into the new firmware
#: ../src/ch-flash.c:358
msgid "Starting the new firmware..."
msgstr "Spouštím nový firmware..."

#. TRANSLATORS: tell the device the firmware is no
#. * longer known working
#: ../src/ch-flash.c:401
msgid "Failed to write the firmware"
msgstr "Nepodařilo se zapsat firmware."

#. TRANSLATORS: now we've written the firmware, we have to
#. * verify it before we tell the device it was successfull
#: ../src/ch-flash.c:411
msgid "Verifying new firmware..."
msgstr "Ověřuji nový firmware..."

#. TRANSLATORS: now write the new firmware chunks
#: ../src/ch-flash.c:440
msgid "Writing new firmware..."
msgstr "Zapisuji nový firmware..."

#. TRANSLATORS: the device failed to come back alive
#: ../src/ch-flash.c:471
msgid "Failed to reboot the ColorHug"
msgstr "Nepodařilo se rebootovat ColorHug"

#. TRANSLATORS: we restart the device in bootloader mode
#: ../src/ch-flash.c:501
msgid "Failed to reset the ColorHug"
msgstr "Nepodařilo se restartovat ColorHug"

#. TRANSLATORS: switch from firmware mode into bootloader mode
#: ../src/ch-flash.c:532
msgid "Resetting into bootloader..."
msgstr "Restartuji do zavaděče..."

#. TRANSLATORS: failed to get the firmware file
#: ../src/ch-flash.c:565
msgid "Failed to get firmware"
msgstr "Nepodařilo se získat firmware"

#. TRANSLATORS: the server gave us an invalid file
#: ../src/ch-flash.c:574
msgid "Firmware has zero size"
msgstr "Firmware má nulovou velikost"

#. TRANSLATORS: the server gave us an invalid file
#: ../src/ch-flash.c:586
msgid "Firmware has incorrect checksum"
msgstr "Firmware má chybný kontrolní součet"

#. TRANSLATORS: the long details about all the updates that
#. * are newer than the version the user has installed
#: ../src/ch-flash.c:674
msgid "Warnings about this update"
msgstr "Varování o této aktualizaci"

#. TRANSLATORS: this is the button text to continue the flash
#. * after showing the user a warning
#: ../src/ch-flash.c:682
msgid "Flash anyway"
msgstr "Přesto nahrát"

#. TRANSLATORS: this is the button text to abort the flash process
#: ../src/ch-flash.c:684
msgid "Do not flash"
msgstr "Nenahrávat"

#. TRANSLATORS: downloading the firmware binary file from a
#. * remote server
#: ../src/ch-flash.c:729
msgid "Downloading update..."
msgstr "Stahování aktualizace..."

#. TRANSLATORS: the user is already running the latest firmware
#: ../src/ch-flash.c:822
msgid "There are no updates available."
msgstr "Žádné aktualizace nejsou k dispozici."

#. TRANSLATORS: the user is running an old firmware version
#: ../src/ch-flash.c:842
msgid "A firmware update is available."
msgstr "Aktualizace firmwaru je k dispozici."

#. TRANSLATORS: the HTTP request failed
#: ../src/ch-flash.c:866
msgid "Failed to get the listing of firmware files"
msgstr "Nepodařilo se získat seznam souborů s firmware"

#. TRANSLATORS: we got an invalid response from the server
#: ../src/ch-flash.c:875
msgid "The firmware listing has zero size"
msgstr "Seznam firmware má nulovou velikost"

#. TRANSLATORS: the XML file was corrupt
#: ../src/ch-flash.c:889
msgid "Failed to parse the update metadata"
msgstr "Nepodařilo se zpracovat informace o aktualizaci"

#. TRANSLATORS: this is test firmware
#: ../src/ch-flash.c:916
msgid "This is a test firmware not intended for general release."
msgstr ""

#. TRANSLATORS: all bets are off
#: ../src/ch-flash.c:918
msgid "This firmware has not been widely tested and may not work as expected."
msgstr "Firmware nebyl dostatečně otestován a nemusí fungovat správně."

#. TRANSLATORS: the device refused our request to mark
#. * it lost or stolen
#: ../src/ch-flash.c:977
msgid "Failed to set the device as lost or stolen"
msgstr "Nepodařilo se nastavit zařízení jako ztracené nebo ukradené"

#. TRANSLATORS: we have a blacklist that we match the device against
#: ../src/ch-flash.c:992
msgid "This ColorHug has been registered as lost or stolen."
msgstr "Tento ColorHug byl nahlášen jako ztracený nebo ukradený."

#. TRANSLATORS: We disable the device so it can't go into
#. * firmware mode without a magic incantation
#: ../src/ch-flash.c:1001
msgid "The device has now been deactivated."
msgstr "Zařízení je nyní deaktivováno."

#. TRANSLATORS: We can tell the user the magic incantation if we've
#. * made a mistake
#: ../src/ch-flash.c:1006
msgid "Please contact <tt>info@hughski.com</tt> for more details."
msgstr "Pro více informací prosím kontaktujte <tt>info@hughski.com</tt>."

#. TRANSLATORS: we failed to get the blacklist from the server
#: ../src/ch-flash.c:1062
msgid "Failed to get device blacklist"
msgstr "Nepodařilo se načíst seznam nepodporovaných zařízení"

#. TRANSLATORS: we've specified a local file
#: ../src/ch-flash.c:1087
msgid "Flashing firmware file..."
msgstr "Nahrávám soubor s firmware..."

