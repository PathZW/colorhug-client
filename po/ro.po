msgid "LCD"
msgstr "LCD"

#. Default cathode ray tube (ask your parents...) matrix
#: ../data/ch-ccmx.ui.h:14
msgid "CRT"
msgstr "CRT"

#. Default projector (beamer etc) matrix
#: ../data/ch-ccmx.ui.h:16
msgid "Projector"
msgstr "Proiector"

#. Default light emitting diode matrix
#: ../data/ch-ccmx.ui.h:18
msgid "LED"
msgstr "LED"

#. TRANSLATORS: device is not connected to the computer
#: ../data/ch-ccmx.ui.h:19 ../src/ch-flash.c:1297
msgid "Please connect your ColorHug"
msgstr "Conectați dispozitivul ColorHug"

#: ../data/ch-flash.ui.h:1
msgid "ColorHug Firmware Updater"
msgstr "Actualizare firmware pentru ColorHug"

#. Button to update to a newer version of the firmware
#: ../data/ch-flash.ui.h:4
msgid "Update!"
msgstr "Actualizează!"

