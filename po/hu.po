msgid "ColorHug Spectro Utility"
msgstr "ColorHug Spectro kisalkalmazás"

#: ../data/ch-spectro-util.ui.h:3
msgid "Auto update:"
msgstr "Automatikus frissítés:"

#: ../data/ch-spectro-util.ui.h:4
msgid "Force Refresh"
msgstr "Frissítés eröltetése"

#: ../data/ch-spectro-util.ui.h:5
msgid "Temperature:"
msgstr "Hőmérséklet:"

#: ../data/ch-spectro-util.ui.h:6
msgid "Interval:"
msgstr "Tartomány:"

#: ../data/ch-spectro-util.ui.h:7
msgid "This shows the raw sensor reading without calibration"
msgstr "Ez mutatja a puszta szenzor érzékelési adatait kalibráció nélkül"

#: ../data/ch-spectro-util.ui.h:8
msgid "Raw Sensor"
msgstr "Puszta szenzor"

#: ../data/ch-spectro-util.ui.h:9
msgid "This shows the dark calibration at 25 degrees C"
msgstr "Ezt mutatja a sötét kalibrációkor 25 celsiuson"

#: ../data/ch-spectro-util.ui.h:10
msgid "Dark Calibration"
msgstr "Sötét kalibráció"

#: ../data/ch-spectro-util.ui.h:11
msgid ""
"This shows the Color Match Function used for the in-device XYZ conversion"
msgstr "Ez mutatja a felhasznált Színhűség funkciót az eszközön belüli XYZ konverzióhoz"

#: ../data/ch-spectro-util.ui.h:12
msgid "Color Match"
msgstr "Színhűség"

#: ../data/ch-spectro-util.ui.h:13
msgid "This shows the temperature calibration factor"
msgstr "Ez mutatja a szín kalibrációs faktort"

#: ../data/ch-spectro-util.ui.h:14
msgid "Temperature Compensation"
msgstr "Hőmérséklet kompenzáció"

#: ../data/ch-spectro-util.ui.h:15
msgid "This shows the calibrated spectra result"
msgstr "Ez mutatja a kalibrált spektra eredményeit"

#: ../data/ch-spectro-util.ui.h:16
msgid "Spectra Result"
msgstr "Spektra eredmények"

