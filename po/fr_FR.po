#: ../data/ch-util.ui.h:13
msgid "Blue"
msgstr "Bleu"

#. Set the multiplier scaling factor
#: ../data/ch-util.ui.h:15
msgid "Multiplier:"
msgstr "Multiplicateur :"

#: ../data/ch-util.ui.h:17
#, no-c-format
msgid "0% (off)"
msgstr "0% (éteint)"

#: ../data/ch-util.ui.h:19
#, no-c-format
msgid "20%"
msgstr "20%"

#: ../data/ch-util.ui.h:21
#, no-c-format
msgid "2%"
msgstr "2%"

#: ../data/ch-util.ui.h:23
#, no-c-format
msgid "100%"
msgstr "100%"

#. Set the time to collect each reading for
#: ../data/ch-util.ui.h:25
msgid "Sample read time:"
msgstr "Temps de lecture de l'échantillon :"

#: ../data/ch-util.ui.h:26
msgid "5ms"
msgstr "5ms"

#: ../data/ch-util.ui.h:27
msgid "50ms"
msgstr "50ms"

#: ../data/ch-util.ui.h:28
msgid "100ms"
msgstr "100ms"

#: ../data/ch-util.ui.h:29
msgid "200ms"
msgstr "200ms"

#: ../data/ch-util.ui.h:30
msgid "Maximum"
msgstr "Maximum"

#: ../data/ch-util.ui.h:31
msgid "Firmware version:"
msgstr "Version du micrologiciel :"

#: ../data/ch-util.ui.h:32
msgid "Serial number:"
msgstr "Numéro de série :"

#. This is the zero-offset when measuring perfect black
#: ../data/ch-util.ui.h:34
msgid "Dark offset:"
msgstr "Point noir :"

#. This is the device calibration matrix
#: ../data/ch-util.ui.h:36
msgid "Calibration:"
msgstr "Étalonnage"

#. This is the time that the last sample took
#: ../data/ch-util.ui.h:38
msgid "Sample duration:"
msgstr "Durée de l'échantillon :"

#. This is the last XYZ color measured by the device
#: ../data/ch-util.ui.h:40
msgid "Last sample:"
msgstr "Dernier échantillon :"

#. The sample mode enables us to switch off the calibration matrix if required
#: ../data/ch-util.ui.h:42
msgid "Sample mode:"
msgstr "Mode de l'échantillon :"

#. A sample is taken without the calibration matrix
#: ../data/ch-util.ui.h:44
msgid "Raw"
msgstr "Brut"

#. The sample is converted with an internal calibration matrix
#: ../data/ch-util.ui.h:46
msgid "With calibration matrix"
msgstr "Avec la matrice d'étalonnage"

#. This is a scale value applied after the reading has been taken
#: ../data/ch-util.ui.h:48
msgid "Pre scale:"
msgstr "Pré-échelle :"

#. This is a scale value applied after the dRGB reading has been converted to
#. XYZ
#: ../data/ch-util.ui.h:50
msgid "Post scale:"
msgstr "Post-échelle :"

#: ../data/ch-util.ui.h:51
msgid "Device State"
msgstr "État de l'appareil"

#. This writes any changed settings to the internal memory
#: ../data/ch-util.ui.h:53
msgid "Write EEPROM"
msgstr "Écrire l'EEPROM"

#. This reads a color using the sensor
#: ../data/ch-util.ui.h:55
msgid "Measure Sample"
msgstr "Mesure de l'échantillon"

#. This measures the perfect black reading and sets it.
#: ../data/ch-util.ui.h:57
msgid "Measure Dark"
msgstr "Mesure du point noir"

#. Calibrate the hardware
#: ../data/ch-util.ui.h:59
msgid "Calibrate"
msgstr "Étalonner"

