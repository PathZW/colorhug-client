msgstr "Użycie domyślnej macierzy daje zadowalające wyniki kalibracji dla większości użytkowników."

#. Default liquid crystal display matrix
#: ../data/ch-ccmx.ui.h:12
msgid "LCD"
msgstr "LCD"

#. Default cathode ray tube (ask your parents...) matrix
#: ../data/ch-ccmx.ui.h:14
msgid "CRT"
msgstr "CRT"

#. Default projector (beamer etc) matrix
#: ../data/ch-ccmx.ui.h:16
msgid "Projector"
msgstr "Projektor"

#. Default light emitting diode matrix
#: ../data/ch-ccmx.ui.h:18
msgid "LED"
msgstr "LED"

#. TRANSLATORS: device is not connected to the computer
#: ../data/ch-ccmx.ui.h:19 ../src/ch-flash.c:1297
msgid "Please connect your ColorHug"
msgstr "Proszę podłączyć urządzenie ColorHug"

#: ../data/ch-flash.ui.h:1
msgid "ColorHug Firmware Updater"
msgstr "Aktualizacja oprogramowania urządzenia ColorHug"

#. Button to update to a newer version of the firmware
#: ../data/ch-flash.ui.h:4
msgid "Update!"
msgstr "Zaktualizuj"

#: ../data/ch-flash.ui.h:5
msgid "Flash new firmware to the device"
msgstr "Zapisuje nowe oprogramowanie sprzętowe w urządzeniu"

#: ../data/ch-flash.ui.h:6
msgid "Firmware version unknown"
msgstr "Nieznana wersja oprogramowania sprzętowego"

#. We are flashing the device, so we have to be careful
#: ../data/ch-flash.ui.h:8
msgid "Do not unplug the device and do not turn off the computer."
msgstr "Nie należy odłączać urządzenia ani wyłączać komputera."

